{
  "id": "high-throughput-64mb",
  "name": "High Throughput (64MB Buffers)",
  "description": "Aggressive tuning for maximum throughput on high-bandwidth links with significant latency (high BDP). Uses 64MB buffers for long-distance, high-capacity connections like intercontinental data transfers.",
  "risk_level": "medium",
  "requires_reboot": false,
  "sysctl": {
    "net.core.default_qdisc": "fq",
    "net.ipv4.tcp_congestion_control": "bbr",
    "net.core.rmem_max": 67108864,
    "net.core.wmem_max": 67108864,
    "net.core.rmem_default": 1048576,
    "net.core.wmem_default": 1048576,
    "net.ipv4.tcp_rmem": "4096 1048576 67108864",
    "net.ipv4.tcp_wmem": "4096 1048576 67108864",
    "net.ipv4.tcp_mtu_probing": 1,
    "net.ipv4.tcp_slow_start_after_idle": 0,
    "net.ipv4.tcp_window_scaling": 1,
    "net.ipv4.tcp_timestamps": 1,
    "net.ipv4.tcp_sack": 1,
    "net.core.netdev_max_backlog": 5000,
    "net.core.somaxconn": 4096
  },
  "qdisc": {
    "type": "fq",
    "interfaces": "default-route"
  },
  "systemd": {
    "ensure_qdisc_service": true
  }
}
