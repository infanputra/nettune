{
  "id": "low-latency",
  "name": "Low Latency Optimized",
  "description": "Optimized for minimal latency scenarios such as gaming, real-time trading, or interactive applications. Uses smaller buffers and aggressive congestion control to reduce queuing delay.",
  "risk_level": "medium",
  "requires_reboot": false,
  "sysctl": {
    "net.core.default_qdisc": "fq_codel",
    "net.ipv4.tcp_congestion_control": "bbr",
    "net.core.rmem_max": 4194304,
    "net.core.wmem_max": 4194304,
    "net.ipv4.tcp_rmem": "4096 87380 4194304",
    "net.ipv4.tcp_wmem": "4096 65536 4194304",
    "net.ipv4.tcp_mtu_probing": 1,
    "net.ipv4.tcp_slow_start_after_idle": 0,
    "net.ipv4.tcp_fastopen": 3,
    "net.ipv4.tcp_low_latency": 1,
    "net.ipv4.tcp_nodelay": 1
  },
  "qdisc": {
    "type": "fq_codel",
    "interfaces": "default-route"
  },
  "systemd": {
    "ensure_qdisc_service": true
  }
}
